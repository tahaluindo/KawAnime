@font-face
  font-family 'Open Sans'
  font-style normal
  src local('Open Sans'),
      local('OpenSans-Regular'),
      url(./assets/open-sans/OpenSans-Regular.ttf) format('truetype'),
      url(./assets/open-sans/OpenSans-SemiBold.ttf) format('truetype'),
      url(./assets/open-sans/OpenSans-SemiBoldItalic.ttf) format('truetype')

  unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
